algoritmo
	declare contador, montante numerico
	montante <- 5
	escreva "O saldo inicial é: ", montante
	para contador <-1 ate 12 faca
	inicio
		montante <- montante * 2
		escreva "Saldo no ", contador, "° mes: ", montante
	fim
	escreva "Ao final de 1 meses terei: ", montante, " reais"
	
fim_algoritmo
